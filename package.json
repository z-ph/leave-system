{
  "name": "leave-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit --project tsconfig.app.json --strict && vue-tsc --noEmit --strict -p tsconfig.app.json",
    "type-check-vue": "vue-tsc --noEmit",
    "type-check-ts": "tsc --noEmit --project tsconfig.app.json --strict",
    "generate:api": "openapi --input openapi.json --output src/api/generated/",
    "gen:api": "npx swagger-typescript-api generate -p ./openapi.yaml -o ./src/api/axios --axios",
    "build:nocheck":"vite build"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@tanstack/vue-query": "^5.90.5",
    "axios": "^1.12.2",
    "element-plus": "^2.11.5",
    "jwt-decode": "^4.0.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^10.5.1",
    "openapi-typescript-codegen": "^0.29.0",
    "swagger-typescript-api": "^13.2.16",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.1.7",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0"
  }
}
